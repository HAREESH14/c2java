#include <stdio.h>
#include <stdlib.h>

int fib(int n);

int fib(int n)
{
  if (n <= 1)
  {
    return n;
  }
  return fib(n - 1) + fib(n - 2);
}



int main()
{
  int results[10];
  for (int i = 0; i < 10; i++)
  {
    results[i] = fib(i);
    printf("fib(%d) = %d\n", i, results[i]);
  }

  int x = 15;
  x += 5;
  x *= 2;
  printf("x = %d\n", x);
  int grade = 2;
  switch (grade)
  {
    case 1:
      printf("A\n");
      break;

    case 2:
      printf("B\n");
      break;

    case 3:
      printf("C\n");
      break;

    default:
      printf("F\n");
      break;

  }

  for (int _fi1 = 0; _fi1 < (sizeof(results) / sizeof(results[0])); _fi1++)
  {
    int v = results[_fi1];
    if (v > 10)
    {
      break;
    }
    printf("%d\n", v);
  }

  return 0;
}


